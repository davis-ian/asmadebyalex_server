generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id      Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  text      String
  completed Boolean
}

model User {
  id      Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email String
  name String
}